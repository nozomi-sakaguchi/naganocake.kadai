<h1>商品一覧</h1>

<table>
    <thead>
        <tr>
            <th>商品ID</th>
            <th>商品名</th>
            <th>税込価格</th>
            <th>ジャンル</th>
            <th>ステータス</th>
        </tr>
    </thead>

    <% @items.each do |item| %>
    <tbody>
        <tr>
            <td></td>
            <td><%= link_to admin_item_path(item.id) do %>
                  <%= item.name %>
                <% end %>
            </td>
        </tr>
    </tbody>
    <% end%>
</table>